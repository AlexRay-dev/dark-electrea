{
  "name": "dark-electrea",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "buildAndUpload": "vite build && scp -r ./dist/* root@server:/srv/frontend",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "axios": "^1.2.1",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "pinia": "^2.0.28",
    "roboto-fontface": "*",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.0.6",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.29.0",
    "eslint-plugin-vue": "^9.8.0",
    "vite": "^3.2.5",
    "vite-plugin-vuetify": "^1.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "parser": "vue-eslint-parser",
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-mutating-props": [
        "off"
      ],
      "key-spacing": [
        "error",
        {
          "beforeColon": false
        }
      ],
      "switch-colon-spacing": "error",
      "comma-spacing": "error",
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  }
}
